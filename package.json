{
  "name": "dokan-wpml",
  "version": "1.1.12",
  "description": "WPML compatibility for dokan plugin",
  "author": "weDevs",
  "license": "GPL",
  "private": true,
  "directories": {
    "build": "build",
    "languages": "languages"
  },
  "files": [
    "build",
    "languages",
    "changelog.txt",
    "package.json",
    "readme.txt",
    "wpml-config.xml",
    "dokan-wpml.php"
  ],
  "scripts": {
    "build": "wp-scripts build",
    "start": "wp-scripts start",
    "packages-update": "wp-scripts packages-update",
    "makepot": "wp i18n make-pot --domain='dokan-wpml' --include='dokan-wpml.php' . languages/dokan-wpml.pot",
    "release": "npm run build && npm run makepot && npm run zip",
    "zip": "node bin/zip"
  },
  "devDependencies": {
    "@wordpress/scripts": "^30.21.0",
    "fs-extra": "^10.1.0",
    "lodash": "^4.17.21"
  },
  "dependencies": {
    "@wordpress/dom-ready": "^4.28.0",
    "chalk": "^4.1.2"
  }
}
